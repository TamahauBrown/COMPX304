interfaces {
    ethernet eth1 {
        address 10.0.34.81/30
        description "to R0"
        duplex auto
        hw-id 00:aa:00:90:00:01
        smp_affinity auto
        speed auto
    }
    loopback lo {
        address 192.168.127.1/32
    }
}
system {
    gateway-address 10.0.34.82
    host-name ISP
    login {
        user vyatta {
            authentication {
                encrypted-password $1$1WtdCQ3x$tEayWu2oe8dwau6SquaBe0
            }
            level admin
        }
    }
    ntp-server 0.vyatta.pool.ntp.org
    package {
        auto-sync 1
        repository supported {
            components main
            distribution stable
            password ""
            url http://packages.vyatta.com/vyatta-supported
            username ""
        }
    }
    syslog {
        global {
            facility all {
                level notice
            }
            facility protocols {
                level debug
            }
        }
    }
    time-zone GMT
}


